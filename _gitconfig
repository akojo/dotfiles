[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
  eol = native
  safecrlf = true
[alias]
  a = add
  au = add -u
  ci = commit
  co = checkout
  dc = diff --cached
  d = diff
  f = fetch
  fa = fetch --all
  l = log --oneline --abbrev-commit --decorate=short --graph --topo-order
  pr = pull --rebase
  r = rebase
  ri = rebase --interactive
  rc = rebase --continue
  root = rev-parse --show-toplevel
  sci = svn dcommit
  sr = svn rebase
  st = status -s
[push]
  default = upstream
[help]
  autocorrect = 1
